{
  "env": {
    "jest": true
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "always"
      }
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "import/extensions": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}